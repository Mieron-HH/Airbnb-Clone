{
	"expo": {
		"name": "Airbnb_Clone",
		"slug": "Airbnb_Clone",
		"description": "An Airbnb clone Expo app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/airbnb-icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/airbnb-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/airbnb-icon.png",
				"backgroundColor": "#000000"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/airbnb-favicon.jpg"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		}
	}
}
